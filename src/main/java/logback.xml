<?xml version="1.0" encoding="UTF-8"?>
<configuration>
<appender name="FILE" class="ch.qos.logback.core.FileAppender">
    <file>/Users/andre/Downloads/logs.txt</file>
    <append>true</append>
    <encoder>
        <pattern>%msg%n</pattern>
